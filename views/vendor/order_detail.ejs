                <div class="white-box">
                    <div class="panel-body">
                        <h3 class="page-title">Order Detail</h3>
                        <hr>
                        <div class="table-responsive">
                            <table class="table product-overview">
                                <thead>
                                    <tr>
                                        <th>Image</th>
                                        <th>Product info</th>
                                        <th>Price</th>
                                        <th>Weight</th>
                                        <th>Quantity</th>
                                        <th style="text-align:center">Total</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for( let i = 0; i < updated.length; i++ ) { %>
                                        <tr>
                                            <td><img src="<%= updated[i].image %>" alt="iMac" width="80"></td>
                                            <td>
                                                <h5 class="font-500"><%= updated[i].name %></h5>
                                            </td>
                                            <td>&#8377;<%= updated[i].price %></td>
                                            <td>
                                                <%= updated[i].weight %> <%= updated[i].unit %>
                                            </td>
                                            <td>
                                                <%= updated[i].quantity %>
                                            </td>
                                            <td align="center" class="font-500">&#8377;<%= updated[i].price * updated[i].quantity %></td>
                                        </tr>
                                    <% } %>
                                </tbody>
                            </table>
